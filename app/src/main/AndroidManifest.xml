<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="net.javango.carcare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="net.javango.carcare.CarListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="net.javango.carcare.CarDetailActivity">
        </activity>
        <activity android:name="net.javango.carcare.ServiceListActivity">
        </activity>
        <activity android:name="net.javango.carcare.ServiceDetailActivity">
        </activity>
    </application>

</manifest>